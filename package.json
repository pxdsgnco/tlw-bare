{
  "name": "tlw-bare",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "analyze": "ANALYZE=true npm run build",
    "lighthouse": "lhci collect --url=http://localhost:3000",
    "lighthouse:ci": "lhci autorun",
    "performance:audit": "npm run build && npm run lighthouse",
    "performance:report": "npm run analyze && npm run lighthouse",
    "performance:check": "node scripts/performance-check.js",
    "performance:monitor": "npm run build && npm start & sleep 5 && npm run performance:check"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.539.0",
    "next": "15.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@lhci/cli": "^0.15.1",
    "@next/bundle-analyzer": "^15.4.7",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "lighthouse": "^12.8.1",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.6",
    "typescript": "^5",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
